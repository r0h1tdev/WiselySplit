#include<iostream>
#include<vector>

using namespace std;

int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		int n, m;
		cin >> n >> m;
		
		if (n == 1 || m == 1)
		{
			cout << n << ' ' << m << '\n';
			continue;
		}
		if (n > 3 || m > 3)
		{
			cout << n << ' ' << m << '\n';
			continue;
		}
		cout << n / 2 + 1 << ' ' << m / 2 + 1 << '\n';
	}
}
